  <div class="col-lg-6">
    <h4>Projetos trabalhados hoje</h4>
    <% user_web.projects.each do |p| %>
        <p><%= p.name %> - <%= TimeWork.worked_today_project p %></p>
    <% end %>
  </div>
</div>