<div class="jumbotron">
  <h1 class="contador-label">00 : 00 : 00</h1>
  <%= raw_correct_form %>
</div>

<div class="row marketing">
  <div class="col-lg-6">
    <h4>Horas trabalhadas hoje</h4>
    <p><%= TimeWork.worked_today user_web%></p>
